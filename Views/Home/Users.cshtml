@model BioNetWork.Models.Users.UsersModel


<h2>List of User</h2>
<br />

<a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Create">Новый пользователь</a>
<table class="table">
    <thead>
        <tr>
            <th>DI</th>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Adress</th>
            <th>DateRegistration</th>
            <th></th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model.UserList)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Email</td>
                <td>@item.Phone</td>
                <td>@item.Adres</td>
                <td>@item.DateRegistration</td>
                <td>
                    <a class="btn btn-primary btn-sm" href="/Users/Delete?id=">Изменить</a>
                    <a class="btn btn-danger btn-sm" href="/Users/Edit?id=">Удалить</a>
                </td>
            </tr>
        }
    </tbody>
</table>